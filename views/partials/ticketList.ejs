<section>
    <h2 class="ml-4 mt-4 text-white  text-center"> <span class="underline">Booked Tickets</span></h2>
    <% if (tickets.length >= 0){ %>
    <div class="card-panel bg-dark p-2">
        <% tickets.forEach(ticket=>{ %>
        <div class=" admin-cat">
            <div class="m-3 bg-white rounded text-center ticket-card">
                <h3><%=ticket.movieCode %></h3>
                <span class="text-capitalize"><%=ticket.name %></span>
                <span class="pl-4 font-weight-bold"><%=ticket.phoneNo %></span>
                <div class="d-flex justify-content-center p-2">
                    <form action="/ticket/deleteMovie" method="POST">
                        <input type="hidden" name="id" value="<%= ticket._id %>">
                        <input type="submit" value="Delete" class="btn btns  btn-sm px-5">
                    </form>
                </div>
            </div>
        </div>
        <% }) %>
    </div>
    <% } else { %>
    <p class="text-white ml-4 mt-4 "><span class="undeline">Ticket not found</span></p>
    <% } %>
</section>
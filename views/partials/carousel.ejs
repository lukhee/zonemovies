
<div id="landingPage" class="carousel slide carousel-fade" style="height: 480px; background-color: rgba(255,0,0,0.1);" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#landingPage" data-slide-to="0" class="active"></li>
        <li data-target="#landingPage" data-slide-to="1"></li>
        <li data-target="#landingPage" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner h-100">
        <div class="carousel-item active h-100">
            <img src="<%= carousel[0] %>" class="d-block w-100  h-100" alt="...">
        </div>
        <%for(var i = 1; i<3; i++) { %>
            <div class="carousel-item h-100">
                <img src="<%= carousel[i] %>" class="d-block w-100  h-100" alt="...">
            </div>
        <% } %>
    </div>
    <a class="carousel-control-prev" href="#landingPage" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#landingPage" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>